{
    "main": {
        "files": [{
            "src": ["**"],
            "dest": "bin/to/",
            "expand": true,
            "cwd": "bin/from"
        }],
        "ignoreInDest": ["t", "*.js"],
        "updateAndDelete": true,
        "verbose": true,
        "pretend": true
    },
    "rename": {
        "src": "bin/somefile.txt",
        "dest": "bin/somefile2.txt"
    }
}